<view class="semp-notice-bar"><view hidden="{{!(showNotice)}}" class="{{['show-box flex-row',round?'round':'']}}" style="{{'background:'+(bgColor)+';'}}"><block s-if="{{icon!=''}}"><view class="icon"><uni-icon vue-id="0301cba5-1" type="{{icon}}" size="30" color="{{setIconColor}}"></uni-icon></view></block><block s-if="{{showType=='slider'}}"><view class="{{['show-info flex-row',icon==''?'no-icon':'']}}"><view data-event-opts="{{[['tap',[['onItemClick',['$event']]]]]}}" class="{{['text-box',rows&&!scrollable?'text-rows':'']}}" style="{{'margin-left:'+(sw+'px')+';'+('color:'+(color)+';')}}" bindtap="__e">{{strText}}</view></view></block><block s-if="{{showType=='scrollTop'}}"><view class="scroll-box" style="{{'color:'+(color)+';'}}"><swiper vertical="true" autoplay="true" circular="true" interval="3000"><block s-for="{{arrayText}}" s-for-item="item" s-for-index="index" s-key="index"><swiper-item><navigator data-event-opts="{{[['tap',[['onItemClick',['$event']]]]]}}" bindtap="__e">{{item}}</navigator></swiper-item></block></swiper></view></block><block s-if="{{showType=='scrollLeft'}}"><view class="scroll-box" style="{{'color:'+(color)+';'}}"><swiper autoplay="true" circular="true" interval="3000"><block s-for="{{arrayText}}" s-for-item="item" s-for-index="index" s-key="index"><swiper-item><navigator data-event-opts="{{[['tap',[['onItemClick',['$event']]]]]}}" bindtap="__e">{{item}}</navigator></swiper-item></block></swiper></view></block></view></view>